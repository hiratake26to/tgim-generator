{
  "name" : "example",
  "channel" : {
    "link_0" : {
      "type" : "PointToPoint",
      "config" : {
        "Delay" : "2ms",
        "Address" : "192.168.1.0/24",
        "DataRate" : "3Mbps"
      }
    },
    "csma_0" : {
      "type" : "Csma",
      "config" : {
        "DataRate" : "1kbps",
        "Delay" : "1ms",
        "Address" : "192.168.2.0/24"
      }
    }
  },

  "node" : {
    "router" : {
      "netifs" : [
        { "connect" : "link_0", "config": {"Address": "0.0.0.1"} },
        { "connect" : "csma_0", "config": {"Address": "auto"} }
      ]
    },
    "sinker" : {
      "netifs" : [
        { "connect" : "link_0", "config": {"Address": "0.0.0.2"} }
      ],
      "apps" : [
        { "load" : "./app/sink", "args" : "--recv any" }
      ]
    },
    "pinger_goal" : {
      "netifs" : [
        { "connect" : "csma_0", "config": {"Address": "auto"} }
      ],
      "apps" : [
        { "load" : "./app/ping", "args" : "--dst 192.168.1.2" }
      ],
      "config" : { "count" : 3 }
    },
    "pinger_0" : {
      "netifs" : [
        { "connect" : "csma_0", "config": {"Address": "auto"} }
      ],
      "apps" : [
        { "load" : "./app/ping", "args" : "--dst 192.168.1.2" }
      ]
    },
    "pinger_1" : {
      "netifs" : [
        { "connect" : "csma_0", "config": {"Address": "auto"} }
      ],
      "apps" : [
        { "load" : "./app/ping", "args" : "--dst 192.168.1.2" }
      ]
    },
    "pinger_2" : {
      "netifs" : [
        { "connect" : "csma_0", "config": {"Address": "auto"} }
      ],
      "apps" : [
        { "load" : "./app/ping", "args" : "--dst 192.168.1.2" }
      ]
    }
  }
}
